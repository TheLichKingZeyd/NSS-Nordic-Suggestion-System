@model NordicDoorSuggestionSystem.Models.MittTeamViewModel

@{
    Layout ="~/Views/Shared/_mittteam.cshtml";
}

<br>
<h1>@Html.DisplayFor(model => model.TeamName)</h1>
<hr>

<h2>Medlemmer</h2>
<hr />

@foreach (var c in Model.EmployeeTeamList)
{
    <div class="row">
        <div class="col-md-3">
            <i>@c.FirstName</i>
            <i> </i>
            <i>@c.LastName</i>
        </div>
    </div>
}
<hr />